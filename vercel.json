{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "public": true,
  "team": "arcade-board-tracker",
  "github": {
    "enabled": true,
    "silent": false,
    "autoAlias": true
  }
}
